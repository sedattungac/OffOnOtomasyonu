    <div class="container text-center">
        <div class="row">
            <div class="col-md-4">

                <button onclick="document.getElementById('id01').style.display='block'" class="btn btn-danger" style="color: white; width:250px; margin-left: 50px; "><b>Kayıt Ol</b></button>

                @Html.Action("Partial1", "Login")


            </div>



            <div class="col-md-4">

                <button onclick="document.getElementById('id02').style.display='block'" class="btn btn-primary" style="color: white; width:250px; margin-left: 50px; "><b>Personel Girişi</b></button>

                <div id="id02" class="modal">
                    <div class="modal-content animate">
                        @using (Html.BeginForm("AdminLogin", "Login", FormMethod.Post))
                        {


                            <div class="imgcontainer">
                                <span onclick="document.getElementById('id02').style.display='none'" class="close" title="Close Modal">&times;</span>
                                <img src="~/182113022_SedatTUNGAÇ/img/personel1.png" alt="Avatar" class="avatar">
                            </div>

                            <div class="container">
                                <label for="email"><b>Personel Mail</b></label>
                                <input type="text" placeholder="Mail Giriniz" name="KullaniciAd" required>

                                <label for="psw"><b>Şifre</b></label>
                                <input type="password" placeholder="Şifrenizi Giriniz" name="Sifre" required>

                                <button type="submit">Giriş Yap</button>
                                <label>
                                    <input type="checkbox" checked="checked" name="remember"> Beni Hatırla
                                </label>
                            </div>

                            <div class="container" style="background-color:#f1f1f1">
                                <button type="button" onclick="document.getElementById('id02').style.display='none'" class="cancelbtn">Cancel</button>
                                <img src="~/Web/images/OFF-ON ORIGINAL_free-file (1).png" style="width: 175px; margin-left:375px;">
                                <span class="psw"> <a href="#">Şifreyi Unuttum?</a></span>
                            </div>
                        }
                    </div>
                </div>

                <script>
                    // Get the modal
                    var modal = document.getElementById('id02');

                    // When the user clicks anywhere outside of the modal, close it
                    window.onclick = function (event) {
                        if (event.target == modal) {
                            modal.style.display = "none";
                        }
                    }
                </script>
            </div>

            <div class="col-md-4">

                <button onclick="document.getElementById('id03').style.display='block'" class="btn btn-success" style="color: white; width:250px;margin-left: 50px; "><b>Müşteri(Cari) Girişi</b></button>

                <div id="id03" class="modal ">
                    <div class="modal-content animate">

                        @using (Html.BeginForm("CariLogin1", "Login", FormMethod.Post))
                        {

                            <div class="imgcontainer">
                                <span onclick="document.getElementById('id03').style.display='none'" class="close" title="Close Modal">&times;</span>
                                <img src="~/182113022_SedatTUNGAÇ/img/sedat1.png" alt="Avatar" class="avatar">
                            </div>

                            <div class="container">
                                <label for="email"><b>Cari Mail</b></label>
                                <input type="text" placeholder="Mail Giriniz" name="CariMail" required>

                                <label for="psw"><b>Şifre</b></label>
                                <input type="password" placeholder="Şifrenizi Giriniz" name="CariSifre" required>



                                <button type="submit">Giriş Yap </button>
                                <label>
                                    <input type="checkbox" checked="checked" name="remember"> Beni Hatırla
                                </label>
                            </div>

                            <div class="container" style="background-color:#f1f1f1">
                                <button type="button" onclick="document.getElementById('id03').style.display='none'" class="cancelbtn">Cancel</button>
                                <img src="~/Web/images/OFF-ON ORIGINAL_free-file (1).png" style="width: 175px; margin-left:375px;">
                                <span class="psw"><a href="#">Şifreyi Unuttum?</a></span>
                            </div>
                        }
                    </div>

                </div>

                <script>
                    // Get the modal
                    var modal = document.getElementById('id03');

                    // When the user clicks anywhere outside of the modal, close it
                    window.onclick = function (event) {
                        if (event.target == modal) {
                            modal.style.display = "none";
                        }
                    }
                </script>
            </div>

